{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "markdoc",
      "camelcase": "Markdoc",
      "title": "Markdoc",
      "scope": "source.markdoc",
      "file-types": [
        "mdoc",
        "md",
        "markdoc"
      ],
      "highlights": "queries/highlights.scm",
      "injection-regex": "^markdoc$",
      "class-name": "TreeSitterMarkdoc",
      "injections": "queries/injections.scm"
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "MIT",
    "description": "This is a tree sitter parser for markdoc",
    "authors": [
      {
        "name": "Shelton Louis",
        "email": "louisshelton0@gmail.com"
      }
    ],
    "links": {
      "repository": "http://github.com/louiss0/tree-sitter-markdoc"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
