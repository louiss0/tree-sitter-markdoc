{
  "name": "tree-sitter-markdoc",
  "version": "0.1.0",
  "description": "Tree-sitter grammar for Markdoc (Zed compatible)",
  "repository": "http://github.com/louiss0/tree-sitter-markdoc",
  "license": "MIT",
  "author": {
    "name": "Shelton Louis",
    "email": "louisshelton0@gmail.com"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "tree_sitter_markdoc"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter": "^0.22.4",
    "tree-sitter-cli": "^0.25.10"
  },
  "peerDependencies": {
    "tree-sitter": "^0.22.4"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "gen": "tree-sitter generate",
    "test": "tree-sitter test",
    "playground": "tree-sitter playground",
    "highlight": "tree-sitter highlight",
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test:node": "node --test bindings/node/*_test.js"
  }
}
