{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "markdoc",
  "rules": {
    "source_file": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "REPEAT",
            "content": {
              "type": "PATTERN",
              "value": "\\n"
            }
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "_block"
                  },
                  {
                    "type": "REPEAT",
                    "content": {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "REPEAT1",
                          "content": {
                            "type": "PATTERN",
                            "value": "\\n"
                          }
                        },
                        {
                          "type": "SYMBOL",
                          "name": "_block"
                        }
                      ]
                    }
                  }
                ]
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "PATTERN",
              "value": "\\n"
            }
          }
        ]
      }
    },
    "_block": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "frontmatter"
        },
        {
          "type": "SYMBOL",
          "name": "heading"
        },
        {
          "type": "SYMBOL",
          "name": "paragraph"
        }
      ]
    },
    "frontmatter": {
      "type": "SEQ",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "PREC",
            "value": 2,
            "content": {
              "type": "STRING",
              "value": "---"
            }
          }
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "yaml_content"
          },
          "named": true,
          "value": "yaml"
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "PREC",
            "value": 2,
            "content": {
              "type": "STRING",
              "value": "---"
            }
          }
        }
      ]
    },
    "yaml_content": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "PATTERN",
            "value": "[^\\n-][^\\n]*\\n"
          },
          {
            "type": "PATTERN",
            "value": "\\n"
          }
        ]
      }
    },
    "heading": {
      "type": "PREC_RIGHT",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "heading_marker",
            "content": {
              "type": "SYMBOL",
              "name": "heading_marker"
            }
          },
          {
            "type": "FIELD",
            "name": "heading_text",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "heading_text"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          }
        ]
      }
    },
    "heading_marker": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "#{1,6}[ \\t]+"
        }
      }
    },
    "heading_text": {
      "type": "PATTERN",
      "value": "[^\\n]+"
    },
    "paragraph": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "text"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "\\n"
                },
                {
                  "type": "SYMBOL",
                  "name": "text"
                }
              ]
            }
          }
        ]
      }
    },
    "text": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "PATTERN",
          "value": "[^\\n]+"
        }
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t\\r]"
    }
  ],
  "conflicts": [
    [
      "source_file"
    ]
  ],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}