{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "markdoc",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "frontmatter"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_block"
          }
        }
      ]
    },
    "frontmatter": {
      "type": "SEQ",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "PREC",
            "value": 1,
            "content": {
              "type": "STRING",
              "value": "---"
            }
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "PREC",
            "value": 1,
            "content": {
              "type": "PATTERN",
              "value": "\\r?\\n"
            }
          }
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "yaml_content"
          },
          "named": true,
          "value": "yaml"
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "PREC",
            "value": 1,
            "content": {
              "type": "STRING",
              "value": "---"
            }
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "TOKEN",
              "content": {
                "type": "PREC",
                "value": 1,
                "content": {
                  "type": "PATTERN",
                  "value": "\\r?\\n"
                }
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "yaml_content": {
      "type": "REPEAT1",
      "content": {
        "type": "PATTERN",
        "value": "[^\\r\\n-][^\\r\\n]*\\r?\\n|\\r?\\n"
      }
    },
    "_block": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "paragraph"
        }
      ]
    },
    "paragraph": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "text"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "\\r?\\n"
                },
                {
                  "type": "SYMBOL",
                  "name": "text"
                }
              ]
            }
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "\\r?\\n"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "text": {
      "type": "PATTERN",
      "value": "[^\\r\\n]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}